{
  "name": "vite-vue3-ts-eslint-prettier",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --mode development",
    "build:hash": "vite build --mode production_hash",
    "build:history": "vite build --mode production_history",
    "lint-fix": "eslint --fix src --ext .ts,.vue,.js,.cjs --ignore-path .gitignore",
    "lint-check": "eslint --no-fix src --ext .ts,.vue,.js,.cjs --ignore-path .gitignore --max-warnings 0",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/lodash": "^4.14.191",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "lodash": "^4.17.21",
    "monaco-editor": "^0.36.0",
    "sass": "^1.58.3",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.12"
  }
}
